{
  "name": "网页编码修改（Charset）",
  "short_name":"FH-Charset",
  "version": "2020.04.07",
  "manifest_version": 2,
  "description": "专用于网页编码的修改，比如：UTF-8、GBK、日文、韩文、西文等等！",
  "icons": {
    "16": "img/fe-16.png",
    "48": "img/fe-48.png",
    "128": "img/fe-128.png"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": false
  },
  "permissions": [ "activeTab", "webRequest", "webRequestBlocking", "contextMenus", "tabs" ],
  "web_accessible_resources": [
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "homepage_url": "https://www.baidufe.com/fehelper"
}